{
    "name": "storinka/invoke-laravel",
    "description": "Invoke Laravel integration plugin.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davyd Kohut",
            "email": "davyd.kohut@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^8|^9",
        "illuminate/support": "^8|^9",
        "phpdocumentor/reflection-docblock": "^5.2",
        "storinka/invoke": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Invoke\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Invoke\\Laravel\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Invoke\\Laravel\\Providers\\InvokeProvider"
            ]
        }
    }
}
